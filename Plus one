class Solution(object):
    def plusOne(self, digits):
        """
        :type digits: List[int]
        :rtype: List[int]
        """
        digits [-1] += 1 
        if digits[-1] != 9 :
            return digits
        else:
            digits[-1] == 9
            digits[-1]=0
            digits[-2] += 1
            return digits